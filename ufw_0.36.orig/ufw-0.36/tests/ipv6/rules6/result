-A ufw6-user-input -p tcp -m multiport --dports 139,445 -d 2a02:2210:12:a:b820:fff:fea2:25d1 -j DROP -m comment --comment 'dapp_Samba'

### tuple ### deny any 23 2a02:2210:12:a:b820:fff:fea2:25d1 any ::/0 in
-A ufw6-user-input -p tcp -d 2a02:2210:12:a:b820:fff:fea2:25d1 --dport 23 -j DROP
-A ufw6-user-input -p udp -d 2a02:2210:12:a:b820:fff:fea2:25d1 --dport 23 -j DROP

### tuple ### allow any any ::/0 any 2001:db8::/32 in
-A ufw6-user-input -s 2001:db8::/32 -j ACCEPT

### END RULES ###

