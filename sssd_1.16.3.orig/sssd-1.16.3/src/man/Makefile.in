@GPO_DEFAULT_ENFORCING_TRUE@GPO_CONDS = ;gpo_default_enforcing
@HAVE_SYSTEMD_UNIT_TRUE@SYSTEMD_CONDS = ;have_systemd
@ADD_FILES_DOMAIN_FALSE@FILES_CONDS = ;no_enable_files_domain
