@BUILD_AUTOFS_TRUE@AUTOFS_CONDS = ;with_autofs
@BUILD_SSH_TRUE@SSH_CONDS = ;with_ssh
@BUILD_PAC_RESPONDER_TRUE@PAC_RESPONDER_CONDS = ;with_pac_responder
@BUILD_IFP_TRUE@IFP_CONDS = ;with_ifp
@BUILD_SECRETS_TRUE@SEC_CONDS = ;with_secrets
@BUILD_SECRETS_TRUE@KCM_CONDS = ;with_kcm
@BUILD_SYSTEMTAP_TRUE@STAP_CONDS = ;with_stap
